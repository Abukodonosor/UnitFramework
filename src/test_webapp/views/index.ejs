
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Demo app template</title>
</head>
<body>
  <div id = "app">
    <%= title %>
  </div>

<script>
const data = {
    userId:32,
    places: [1,2,3],
    priceRange: "something unusaual",
    filterSchema: { newKey: "goOn", secondKey: 2}
};

fetch('/airplane/by/card', {
    method: "POST",
    body: JSON.stringify({ 
        data: JSON.stringify({
          ...data
        })
    }),
    headers: {
        // 'Accept': 'application/json, text/plain, */*',
        'Content-Type': 'application/json',
    },
})
.then(response => response.json())
.then(data => {
    console.log(data);
})
.catch(err=>{
  console.error(err);
})
.finally(()=>{
    console.log("register block");
});;

</script>

</body>
